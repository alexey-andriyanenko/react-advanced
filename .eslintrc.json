{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "react",
        "prettier",
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
        "prettier/prettier": "error",
        "@typescript-eslint/ban-ts-comment": 1
    }
}
